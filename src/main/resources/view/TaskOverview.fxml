<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="TOP_CENTER" styleClass="display" type="VBox" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="listView" focusTraversable="false" prefHeight="600.0" prefWidth="600.0" />
            <VBox fx:id="messageOverlay" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="450.0" prefWidth="450.0">
               <children>
                  <Label fx:id="icon" text="Icon" />
                  <Label fx:id="greeting" text="Hi" wrapText="true" />
                  <Label fx:id="message" text="Message" wrapText="true" />
               </children>
            </VBox>
         </children>
      </StackPane>
      <Label fx:id="feedbackLabel" focusTraversable="false" text="Feedback" VBox.vgrow="ALWAYS" />
   </children>
</fx:root>
